{
    "name": "Memo App",
    "description": "Save your links with ease",
    "version": "0.0.1",
    "permissions": ["tabs","activeTab","alarms", "notifications", "storage", "management"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
      "default_icon": "slon.png",
      "default_title": "Memo App",
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    },
    "manifest_version": 2
  }